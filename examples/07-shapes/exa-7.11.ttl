@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d: <http://nobel.org/data#> . 
@prefix o: <http://nobel.org/ont#> .
@prefix v: <http://nobel.org/val#> .

v:NobelPrizeShape a sh:NodeShape ;
  sh:targetClass o:NobelPrize ; 
  sh:property v:NobelPrizePrevInvNextShape .
  
v:NobelPrizePrevInvNextShape a v:PropertyShape ;
  sh:path [ 
    sh:alternativePath ( 
      o:prev 
      [ sh:inversePath o:next ]  
    )
  ] ;
  sh:maxCount 1 .